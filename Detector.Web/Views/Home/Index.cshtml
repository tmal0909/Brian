@model IEnumerable<IntrudeRecordEntity>

<div class="text-center">
    <h4 class="mb-4">入侵紀錄</h4>
</div>

<div>
    <table class="table table-dark">
        <thead>
            <tr>
                <th>#</th>
                <th>入侵時間</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.IntrudeTime.ToUniversalTime()</td>
                </tr>
            }
        </tbody>
    </table>
</div>

@section Scripts{
    <script>
        init(@ViewBag.MaxId);
    </script>
}